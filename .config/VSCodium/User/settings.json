{
    "diffEditor.ignoreTrimWhitespace": true,
    //"diffEditor.renderSideBySide": false,
    "editor.rulers": [
        {
            "column": 80,
            "color": "#234"
        },
        {
            "column": 40,
            "color": "#124"
        }
    ],
    "editor.minimap.enabled": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "update.showReleaseNotes": false,
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "statusBarItem.remoteHoverBackground": "#1fa81f",
        "statusBarItem.remoteBackground": "#0db84e",
        "quickInputList.focusBackground": "#3133b6",
        "menubar.selectionBackground": "#3133b6",
        "terminal.tab.activeBorder": "#0db84e",
        "menu.selectionBackground": "#3133b6",
        "activityBar.activeBorder": "#3133b6",
        "panelTitle.activeBorder": "#3133b6",
        "button.hoverBackground": "#4d33ac",
        "tab.activeBorderTop": "#3133b6",
        "button.background": "#3133b6",
        "focusBorder": "#3133b6",
    },
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.clang_format_style": "file:/home/usrj/.config/clang-format",
    "makefile.configureOnOpen": true,
    "git.autoRepositoryDetection": "subFolders",
    "git.openRepositoryInParentFolders": "never",
    "git.openAfterClone": "whenNoFolderOpen",
    //"git.terminalAuthentication": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "NOTE",
        "FIX"
    ],
    "todo-tree.highlights.defaultHighlight": {
        "type": "text"
    },
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "background": "#f002",
            "foreground": "red"
        },
        "HACK": {
            "background": "#FFA50020",
            "foreground": "orange"
        },
        "FIXME": {
            "background": "#ff02",
            "foreground": "yellow"
        },
        "TODO": {
            "background": "#0802",
            "foreground": "green"
        },
        "XXX": {
            "background": "#008b8b20",
            "foreground": "#008b8b"
        },
        "[ ]": {
            "background": "#8082",
            "foreground": "#808"
        },
        "[x]": {
            "background": "#EE82EE20",
            "foreground": "violet"
        },
        "NOTE": {
            "background": "#AAA2",
            "foreground": "#AAA"
        },
        "FIX": {
            "background": "#FFF59A20",
            "foreground": "#FFF59A"
        }
    },
    "todo-tree.general.statusBar": "current file",
    "todo-tree.tree.showCurrentScanMode": false,
    "todo-tree.tree.hideTreeWhenEmpty": true,
    "todo-tree.tree.buttons.refresh": false,
    "todo-tree.tree.buttons.expand": false,
    "hexeditor.showDecodedText": true,
    "hexeditor.columnWidth": 32,
    "nasm.outputFormat": "elf64",
    "nasm.extraFlags": [
        "-Ilib/snesapu/SNES/SNESAPU",
        "-Ilib/snesapu"
    ],
}
